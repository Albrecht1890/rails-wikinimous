<h1>Wikinimous</h1>

<h3 class="add-art"><%= link_to 'Add an article', new_article_path %></h3>

<ul style="list-style-type: none;">
  <% @articles.each do |article| %>
  <li>
    <h4><strong><%= link_to "#{article.title}", article_path(article) %></strong></h4>
    <p> <%= article.content %></p>
  </li>
  <% end %>
</ul>


